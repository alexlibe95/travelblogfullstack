{
  "name": "travelblog-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run sass:build && npm run api:generate",
    "start": "ng serve",
    "prebuild": "npm run sass:build && npm run api:generate",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.{ts,js,html,scss,css,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,html,scss,css,json,md}\"",
    "sass:watch": "sass --watch ./scss/custom.scss ./css/custom.css",
    "sass:build": "sass ./scss/custom.scss ./css/custom.css",
    "generate:api": "tsx scripts/generate-api-client.ts",
    "generate:services": "tsx scripts/generate-services.ts",
    "api:generate": "cd ../backend && npm run export:openapi && cd ../frontend && npm run generate:api && npm run generate:services"
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@fortawesome/angular-fontawesome": "^4.0.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "parse": "^8.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "angular-eslint": "21.1.0",
    "eslint": "^9.39.1",
    "jsdom": "^27.1.0",
    "@types/node": "^24.10.4",
    "openapi-typescript": "^7.4.2",
    "openapi-fetch": "^0.9.3",
    "sass": "^1.97.2",
    "prettier": "^3.7.4",
    "tsx": "^4.19.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.47.0",
    "vitest": "^4.0.8"
  }
}
